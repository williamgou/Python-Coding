import turtle as t

t.penup()
t.goto(-70,0)
t.pencolor('blue')
t.pensize(5)
t.pendown()
t.circle(50)

t.penup()
t.goto(0,0)
t.pencolor('black')
t.pensize(5)
t.pendown()
t.circle(50)

t.penup()
t.goto(70,0)
t.pencolor('red')
t.pensize(5)
t.pendown()
t.circle(50)

t.penup()
t.goto(-35,-70)
t.pencolor('yellow')
t.pensize(5)
t.pendown()
t.circle(50)

t.penup()
t.goto(35,-70)
t.pencolor('green')
t.pensize(5)
t.pendown()
t.circle(50)

t.done()
